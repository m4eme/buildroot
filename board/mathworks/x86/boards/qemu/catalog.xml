<board name="qemu" platform="x86">
    <default>
        <app name="default">
        </app>
        <handoff dir="handoff"/>
    </default>
    <image name="qemu">
		<app name="axilite">
			<dts file="axilite.dts"/>
        </app>
		<app name="axistream">
			<dts file="axistream.dts"/>
        </app>
		<app name="system">
			<dts file="system.dts"/>
        </app>
		<app name="xilinx-pcie-bridge">
			<dts file="xilinx-pcie-bridge.dts"/>
        </app>
    </image>
</board>
